{
  "version": "5.0.0-*",
  "dependencies": {
    "Grace": "5.0.0-*"
  },

  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0",
        "System.Web.Routing": "4.0.0.0"
      },
      "dependencies": {
        "Microsoft.AspNet.Mvc": "4.0.30506.0",
        "Microsoft.AspNet.Razor": "2.0.30506.0",
        "Microsoft.AspNet.WebPages": "2.0.30506.0",
        "Microsoft.Web.Infrastructure": "1.0.0"
      }
    }
  },
  "packOptions": {
    "licenseUrl": "https://github.com/ipjohnson/Grace/blob/master/License.md",
    "projectUrl": "https://github.com/ipjohnson/Grace",
    "releaseNotes": "https://github.com/ipjohnson/Grace/blob/master/Changes-For-5.0.md",
    "repository": {
      "type": "git",
      "url": "https://github.com/ipjohnson/Grace"
    },
    "requireLicenseAcceptance": false,
    "summary": "Grace.MVC4 is a companion library for using Grace in MVC4",
    "tags": []
  },
  "buildOptions": {
    "compile": [ "../Grace.MVC5/DependencyInjection/**", "../Grace.MVC5/Extensions/**" ],
    "xmlDoc": true
  }
}
